; https://github.com/cakephp/cakephp/blob/72fc4cc8ec2f46a96f3bf8086f6b043e12a5c6d5/.editorconfig

root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.bat]
end_of_line = crlf

[*.yml]
indent_size = 2

[Makefile]
indent_style = tab
